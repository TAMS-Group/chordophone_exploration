<?xml version="1.0"?>
<launch>
  <arg name="recording" default="false" />

  <include unless="$(arg recording)" file="$(dirname)/microphone.launch">
    <arg name="play" value="false" />
  </include>

  <include file="$(dirname)/play_audio_topic.launch" />

  <include file="$(dirname)/detect_onset.launch" />
  <include file="$(dirname)/project_onsets.launch" />

  <include file="$(dirname)/detect_pluck.launch" />
  <include file="$(dirname)/project_plucks.launch" />

  <node name="forward_dynamic_parameters_to_pluck_projector"
        pkg="tams_pr2_guzheng"
        type="forward_dynamic_reconfigure_parameters.py"
        ns="guzheng">
    <remap from="source" to="onset_projector" />
    <remap from="follower" to="pluck_projector" />
  </node>
  
<!--
  <group ns="guzheng">
  </group>
-->

</launch>
